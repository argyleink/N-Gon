#demo
  max-width:200px
  height:200px
  
  @media phone
    max-width:400px
    height:400px
  
  @media tablet
    max-width:600px
    height:600px
    
  @media desktop
    max-width:800px
    height:800px
  
  // ipad
  @media (min-device-width: 768px) and (max-device-width: 1024px)
    max-width:450px
    height:450px